<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="application-standalone"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="ru.taximaxim.codekeeper.mainapp.ApplicationStandalone">
         </run>
      </application>
   </extension>
   <extension
         id="product-codekeeper-standalone"
         point="org.eclipse.core.runtime.products">
      <product
            application="ru.taximaxim.codekeeper.mainapp.application-standalone"
            name="pgCodeKeeper standalone">
         <property
               name="appName"
               value="pgCodeKeeper standalone">
         </property>
      </product>
   </extension>
</plugin>
