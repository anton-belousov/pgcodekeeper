<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="id1"
         point="org.eclipse.e4.workbench.model">
      <processor
            apply="always"
            beforefragment="false"
            class="ru.taximaxim.codekeeper.ui.addons.AddonsProcessor">
      </processor>
   </extension>
   <extension
         point="org.eclipse.ui.contexts">
      <context
            id="ru.taximaxim.codekeeper.ui.pgCodeKeeper"
            name="pgCodeKeeper"
            parentId="org.eclipse.ui.contexts.window">
      </context>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="ru.taximaxim.codekeeper.ui.command.Diff"
            contextId="ru.taximaxim.codekeeper.ui.pgCodeKeeper"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+D">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="ru.taximaxim.codekeeper.ui.categoryPgCodeKeeper"
            name="pgCodeKeeper">
      </category>
      <command
            categoryId="ru.taximaxim.codekeeper.ui.categoryPgCodeKeeper"
            defaultHandler="ru.taximaxim.codekeeper.ui.handlers.Diff"
            id="ru.taximaxim.codekeeper.ui.command.Diff"
            name="%command.commandname.10">
      </command>
      <command
            categoryId="ru.taximaxim.codekeeper.ui.categoryPgCodeKeeper"
            defaultHandler="ru.taximaxim.codekeeper.ui.handlers.OpenEditor"
            id="ru.taximaxim.codekeeper.ui.openeditor"
            name="Open in Project Editor">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               label="%menu.label.3"
               mnemonic="P">
            <command
                  commandId="ru.taximaxim.codekeeper.ui.command.Diff"
                  label="%handledmenuitem.label.6"
                  mnemonic="D"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="ru.taximaxim.codekeeper.ui.toolbarPgCodeKeeper">
            <command
                  commandId="ru.taximaxim.codekeeper.ui.command.Diff"
                  icon="icons/compare_view.gif"
                  style="push"
                  tooltip="%handledtoolitem.tooltip.3">
            </command>
         </toolbar>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:ru.taximaxim.codekeeper.ui.popupmenu.project">
         <command
               commandId="ru.taximaxim.codekeeper.ui.command.Diff"
               label="%handledmenuitem.label.15"
               mnemonic="D"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.ui.navigator.ProjectExplorer#PopupMenu?after=additions">
         <command
               commandId="ru.taximaxim.codekeeper.ui.openeditor"
               label="Open in Project Editor"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeMenuSelection">
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <test
                           property="org.eclipse.core.resources.projectNature"
                           value="ru.taximaxim.codekeeper.ui.nature">
                     </test>
                     <adapt
                           type="org.eclipse.core.resources.IProject">
                     </adapt>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.jdt.ui.PackageExplorer">
         <command
               commandId="ru.taximaxim.codekeeper.ui.openeditor"
               label="Open in Project Editor"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeMenuSelection">
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <test
                           property="org.eclipse.core.resources.projectNature"
                           value="ru.taximaxim.codekeeper.ui.nature">
                     </test>
                     <adapt
                           type="org.eclipse.core.resources.IProject">
                     </adapt>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="ru.taximaxim.codekeeper.ui.categoryPgCodeKeeperDevtest"
            name="pgCodeKeeper - Devtest"
            parentCategory="ru.taximaxim.codekeeper.ui.categoryPgCodeKeeper">
      </category>
      <e4view
            category="ru.taximaxim.codekeeper.ui.categoryPgCodeKeeperDevtest"
            class="ru.taximaxim.codekeeper.ui.parts.TestPart"
            id="ru.taximaxim.codekeeper.ui.part.TestPart"
            name="%part.label.4"
            restorable="false">
      </e4view>
      <e4view
            category="ru.taximaxim.codekeeper.ui.categoryPgCodeKeeperDevtest"
            class="com.opcoach.e4.contextExplorer.parts.ContextExplorerPart"
            id="ru.taximaxim.codekeeper.ui.part.CtXp"
            name="%part.label.5"
            restorable="false">
      </e4view>
      <category
            id="ru.taximaxim.codekeeper.ui.categoryPgCodeKeeper"
            name="pgCodeKeeper">
      </category>
      <e4view
            category="ru.taximaxim.codekeeper.ui.categoryPgCodeKeeper"
            class="ru.taximaxim.codekeeper.ui.parts.ProjectExplorer"
            icon="icons/tree_explorer.gif"
            id="ru.taximaxim.codekeeper.ui.part.ProjectExplorer"
            name="%part.label.1"
            restorable="false">
      </e4view>
      <e4view
            category="ru.taximaxim.codekeeper.ui.categoryPgCodeKeeper"
            class="ru.taximaxim.codekeeper.ui.parts.Welcome"
            icon="icons/welcome16.gif"
            id="ru.taximaxim.codekeeper.ui.part.Welcome"
            name="%part.label.2"
            restorable="false">
      </e4view>
      <e4view
            category="ru.taximaxim.codekeeper.ui.categoryPgCodeKeeper"
            class="ru.taximaxim.codekeeper.ui.parts.Console"
            icon="icons/console_view.gif"
            id="ru.taximaxim.codekeeper.ui.part.Console"
            name="%part.label.3"
            restorable="false">
      </e4view>
      <e4view
            allowMultiple="true"
            category="ru.taximaxim.codekeeper.ui.categoryPgCodeKeeper"
            class="ru.taximaxim.codekeeper.ui.parts.SqlEditorDescr"
            icon="icons/file_obj.gif"
            id="ru.taximaxim.codekeeper.ui.partdescriptor.SQLEditorDescr"
            name="%partdescriptor.label.1"
            restorable="false">
      </e4view>
      <e4view
            allowMultiple="false"
            category="ru.taximaxim.codekeeper.ui.categoryPgCodeKeeper"
            class="ru.taximaxim.codekeeper.ui.parts.CommitPartDescr"
            icon="icons/ball_blue.png"
            id="ru.taximaxim.codekeeper.ui.partdescriptor.Sync"
            name="%partdescriptor.label.2"
            restorable="false">
      </e4view>
      <e4view
            allowMultiple="false"
            category="ru.taximaxim.codekeeper.ui.categoryPgCodeKeeper"
            class="ru.taximaxim.codekeeper.ui.parts.DiffPartDescr"
            icon="icons/ball_red.png"
            id="ru.taximaxim.codekeeper.ui.partdescriptor.Diff"
            name="%partdescriptor.label.3"
            restorable="false">
      </e4view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="ru.taximaxim.codekeeper.ui.perspectives.MainPersp"
            icon="icons/app_icon.png"
            id="ru.taximaxim.codekeeper.ui.perspective.Main"
            name="%perspective.label.1">
      </perspective>
      <perspective
            class="ru.taximaxim.codekeeper.ui.perspectives.DevtestPersp"
            icon="icons/beta_violet.png"
            id="ru.taximaxim.codekeeper.ui.perspective.testbs"
            name="%perspective.label.2">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="ru.taximaxim.codekeeper.ui.prefs.GeneralPrefPage"
            id="ru.taximaxim.codekeeper.ui.general"
            name="pgCodeKeeper">
      </page>
      <page
            category="ru.taximaxim.codekeeper.ui.general"
            class="ru.taximaxim.codekeeper.ui.prefs.DbStorePrefPage"
            id="ru.taximaxim.codekeeper.ui.dbstore"
            name="%page.name.dbstore">
      </page>
      <page
            category="ru.taximaxim.codekeeper.ui.general"
            class="ru.taximaxim.codekeeper.ui.prefs.GitPrefPage"
            id="ru.taximaxim.codekeeper.ui.git"
            name="%page.name.gitssh">
      </page>
      <page
            category="ru.taximaxim.codekeeper.ui.general"
            class="ru.taximaxim.codekeeper.ui.prefs.IgnoredObjectsPrefPage"
            id="ru.taximaxim.codekeeper.ui.ignoredobjects"
            name="%page.name.ignored_objects">
      </page>
      <page
            category="ru.taximaxim.codekeeper.ui.general"
            class="ru.taximaxim.codekeeper.ui.prefs.CommitPrefPage"
            id="ru.taximaxim.codekeeper.ui.commitprefpage"
            name="%page.name.commit_pref_page">
      </page>
      <page
            category="ru.taximaxim.codekeeper.ui.general"
            class="ru.taximaxim.codekeeper.ui.prefs.DbUpdatePrefPage"
            id="ru.taximaxim.codekeeper.ui.dbupdateprefpage"
            name="%page.name.db_update_pref_page">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="ru.taximaxim.codekeeper.ui.pgcodekeepercategory"
            class="ru.taximaxim.codekeeper.ui.pgdbproject.NewProjWizard"
            id="ru.taximaxim.codekeeper.ui.newprojwizard"
            name="New Project"
            project="true">
      </wizard>
      <category
            id="ru.taximaxim.codekeeper.ui.pgcodekeepercategory"
            name="PgCodekeeper">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="ru.taximaxim.codekeeper.ui.editors.ProjectEditor"
            default="false"
            id="ru.taximaxim.codekeeper.ui.projecteditor"
            name="Project Editor">
      </editor>
   </extension>
   <extension
         id="ru.taximaxim.codekeeper.ui.nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="ru.taximaxim.codekeeper.ui.natures.ProjectNature">
         </run>
      </runtime>
   </extension>
   <extension
         point="org.eclipse.ui.console.consoleFactories">
      <consoleFactory
            class="ru.taximaxim.codekeeper.ui.parts.ConsoleFactory"
            label="PgCodekeeper console">
      </consoleFactory>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="ru.taximaxim.codekeeper.ui.prefs.PreferenceInitializer">
      </initializer>
   </extension>

</plugin>
