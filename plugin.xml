<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="id1"
         point="org.eclipse.e4.workbench.model">
      <processor
            apply="always"
            beforefragment="false"
            class="ru.taximaxim.codekeeper.ui.addons.AddonsProcessor">
      </processor>
   </extension>
   <extension
         point="org.eclipse.ui.contexts">
      <context
            id="ru.taximaxim.codekeeper.ui.pgCodeKeeper"
            name="pgCodeKeeper"
            parentId="org.eclipse.ui.contexts.window">
      </context>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="ru.taximaxim.codekeeper.ui.command.LoadProj"
            contextId="ru.taximaxim.codekeeper.ui.pgCodeKeeper"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+O">
      </key>
      <key
            commandId="ru.taximaxim.codekeeper.ui.command.OpenLast"
            contextId="ru.taximaxim.codekeeper.ui.pgCodeKeeper"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+O">
      </key>
      <key
            commandId="ru.taximaxim.codekeeper.ui.command.CloseActiveProj"
            contextId="ru.taximaxim.codekeeper.ui.pgCodeKeeper"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+W">
      </key>
      <key
            commandId="ru.taximaxim.codekeeper.ui.command.CloseEditor"
            contextId="ru.taximaxim.codekeeper.ui.pgCodeKeeper"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+W">
      </key>
      <key
            commandId="ru.taximaxim.codekeeper.ui.command.NewProj"
            contextId="ru.taximaxim.codekeeper.ui.pgCodeKeeper"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+N">
      </key>
      <key
            commandId="ru.taximaxim.codekeeper.ui.command.Diff"
            contextId="ru.taximaxim.codekeeper.ui.pgCodeKeeper"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+D">
      </key>
      <key
            commandId="ru.taximaxim.codekeeper.ui.command.ProjProps"
            contextId="ru.taximaxim.codekeeper.ui.pgCodeKeeper"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M3+P">
      </key>
   </extension>
